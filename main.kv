<ModalPriceDetails>:
    id: popup
    canvas:
        Color:
            rgba: 1,1,1,1
        RoundedRectangle:
            radius: [20]
            size: self.size
            pos: self.pos
    auto_dismiss: False
    background: ''
    background_color: 1,1,1,0
    size_hint: .8,.9
    pos_hint: {'center_x': .5, 'center_y': .5}

    MDRelativeLayout:

        BillCloseButton:
            size: 50,50
            size_hint: None,None
            pos_hint: {'center_x':.9,'center_y': 1}
            md_bg_color: app.theme_cls.primary_color
            radius: [40]
            elevation: 10

            MDIconButton:
                icon: 'close'
                pos_hint: {'center_x': .5,'center_y': .5}
                user_font_size: 25
                on_press: self.user_font_size = 20
                on_release: 
                    self.user_font_size = 25
                    popup.dismiss()

        TextInput:
            id: textinput
            hint_text: 'Search in Products'
            size_hint: .9,.06
            pos_hint: {'center_x': .48,'center_y': .9}
            on_text: app.search_price_details_screen(self,self.text)

        FocusMDIconButton:
            icon: 'close'
            pos_hint: {'center_x': .96,'center_y': .9}
            on_release: textinput.text = ''

        MDDropDownItem:
            id: drop_item_sort
            pos_hint: {'center_x': .12,'center_y': .83}
            text: 'Search in'
            on_release: app.open_drop_down_price_details(self)
            theme_text_color: 'Custom'
            text_color: 1,0,0,1
            
        MDLabel:
            id: sort_label_price_details
            text: 'Products'
            size_hint_x: .2
            size_hint_y: .05
            pos_hint: {'center_x': .32, 'center_y': .83}

        MDLabel:
            id: show_more_lab
            text: 'Show More'
            size_hint_x: .2
            size_hint_y: .05
            color: 1,0,0,1
            pos_hint: {'center_x': .85, 'center_y': .83}

        FocusMDCheckbox:
            id: show_more_check
            size_hint: None, None
            size: 48, 48
            pos_hint: {'center_x': .9, 'center_y': .83}
            on_active: app.checkbox_active_show_more_price_details(*args)

        ScrollView:
            size_hint: .95,.74
            pos_hint: {'center_x': .5,'center_y': .4}

            RVPriceDetails:
                id: rv
                scroll_type: ['bars']
                smooth_scroll_end: 20
                bar_width: 7


