<RVBackup>:
    viewclass: 'RecycleListItemBackup'
    RecycleBoxLayout:
        default_size: None, 80
        default_size_hint: 1, None
        size_hint_y: None
        spacing: 2
        height: self.minimum_height
        orientation: 'vertical'

<ModalPopularDay>:
    id: popup
    canvas:
        Color:
            rgba: 1,1,1,1
        RoundedRectangle:
            radius: [20]
            size: self.size
            pos: self.pos
    auto_dismiss: False
    background: ''
    background_color: 1,1,1,0
    size_hint: .5,.2
    pos_hint: {'center_x': .5, 'center_y': .5}

    MDRelativeLayout:

        BillCloseButton:
            size: 50,50
            size_hint: None,None
            pos_hint: {'center_x':.9,'center_y': 1}
            md_bg_color: app.theme_cls.primary_color
            radius: [40]
            elevation: 10

            MDIconButton:
                icon: 'close'
                pos_hint: {'center_x': .5,'center_y': .5}
                user_font_size: 25
                on_press: self.user_font_size = 20
                on_release: 
                    self.user_font_size = 25
                    popup.dismiss()

        MDLabel:
            text: 'Show Grid '
            halign: 'left'
            size_hint: 1,.05
            pos_hint: {'x': .05,'center_y': .5}

        MDCheckbox:
            id: grid
            size_hint: None, None
            size: 48, 48
            active: True
            pos_hint: {'x': .25, 'center_y': .5}

        FocusMDRaisedButton:
            text: 'Plot'
            pos_hint: {'x': .7, 'center_y': .3}
            on_release: app.popular_day(grid)

