#:import utils kivy.utils

<RecycleListItemBackup>:
    pos_hint: {"center_x": .5, "center_y": .5}
    size_hint: 1,None
    elevation: 5
    spacing: 4
    _height: 80

    MDRelativeLayout:
        size_hint: 1,1

        MDLabel:
            size_hint: .7,.3
            pos_hint: {"x": .05, "center_y": .8}
            halign: 'left'
            font_size: 15
            text: root.text
            valign: 'center'

        MDLabel:
            size_hint: .7,.3
            pos_hint: {"x": .05, "center_y": .5}
            halign: 'left'
            font_size: 14
            text: root.sec_text
            valign: 'center'
            theme_text_color: 'Secondary'

        MDLabel:
            size_hint: .7,.3
            pos_hint: {"x": .05, "center_y": .2}
